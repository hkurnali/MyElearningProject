@model MyElearningProject.DAL.Entities.Contact
@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<h2>İletişim Güncelleme Sayfası</h2>

<form method="post">

    @Html.HiddenFor(x => x.ContactID, new { @class = "form-control" })
    <br />
    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    <label>Adres</label>
    @Html.TextBoxFor(x => x.Address, new { @class = "form-control" })
    <br />
    <label>Tel</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
    <br />
    <label>Mail</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Harita</label>
    @Html.TextBoxFor(x => x.MapURL, new { @class = "form-control" })
    <br />
   
    <br />
   
    <br />


    <button class="btn btn-primary">Kaydet</button>
</form>


